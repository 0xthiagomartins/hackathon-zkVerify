# Chaves Privadas
PRIVATE_KEY=                  # Chave privada da conta de deploy
DEPLOYER_ADDRESS=            # Endereço da conta de deploy

# RPCs
TARAXA_RPC_URL=             # URL do RPC da Taraxa (ex: https://rpc.testnet.taraxa.io)
ZKVERIFY_RPC_URL=           # URL do RPC do zkVerify

# API Keys
ETHERSCAN_API_KEY=          # API key do Etherscan (para verificação de contratos)
TARAXA_API_KEY=             # API key da Taraxa (se necessário)

# Configurações de Deploy
DEPLOY_NETWORK=             # Nome da rede para deploy (taraxa_testnet, zkverify_testnet)

# Endereços de Contratos (preencher após deploy)
VERIFIER_ADDRESS=           # Endereço do contrato verificador Noir
ZKCHECKIN_ADDRESS=         # Endereço do contrato ZkCheckin

# Configurações de Gas
GAS_PRICE=                 # Preço do gas em wei (opcional)
GAS_LIMIT=                 # Limite de gas para transações (opcional)

# Configurações de API Backend
API_PORT=8000              # Porta para a API FastAPI
API_HOST=0.0.0.0          # Host para a API FastAPI
CORS_ORIGINS=["http://localhost:3000"] # Origins permitidos para CORS

# Configurações de Debug
DEBUG=true                 # Modo debug (true/false)
LOG_LEVEL=INFO            # Nível de logging (INFO, DEBUG, ERROR) 